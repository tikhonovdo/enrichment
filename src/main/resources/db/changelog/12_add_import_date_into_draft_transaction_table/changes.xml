<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet id="matching-draft_transaction-add-import_date" author="chitzkoy">
        <addColumn schemaName="matching" tableName="draft_transaction" >
            <column name="import_date" type="timestamp" defaultValue="now()" remarks="Дата импорта записи" >
                <constraints nullable="true" />
            </column>
        </addColumn>
    </changeSet>

</databaseChangeLog>